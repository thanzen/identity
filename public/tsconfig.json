{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "preserve",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "files": [
        "events/events.ts",
        "libs/typings/amazeui-react.d.ts",
        "libs/typings/es6-promise.d.ts",
        "libs/typings/fixed-data-table.d.ts",
        "libs/typings/react-bootstrap.d.ts",
        "libs/typings/react/classnames.d.ts",
        "libs/typings/react/history.d.ts",
        "libs/typings/react/react-addons-update.d.ts",
        "libs/typings/react/react-dom.d.ts",
        "libs/typings/react/react-router.d.ts",
        "libs/typings/react/react.d.ts",
        "libs/typings/redux/react-redux.d.ts",
        "libs/typings/redux/redux.d.ts",
        "libs/typings/redux/reselect.d.ts",
        "libs/typings/whatwg-fetch.d.ts",
        "src/actions/index.ts",
        "src/components/index.ts",
        "src/context.ts",
        "src/eventType.ts",
        "src/history.ts",
        "src/models/Xpath.ts",
        "src/pages/index.ts",
        "src/reducers/index.ts",
        "src/utils/objectUtil.ts",
        "src/utils/stringUtils.ts",
        "src/utils/utils.ts",
        "src/BaseComponent.tsx",
        "src/components/extractor/Index.tsx",
        "src/components/extractor/SearchBar.tsx",
        "src/components/extractor/Table.tsx",
        "src/components/Header.tsx",
        "src/containers/Index.tsx",
        "src/containers/public/Index.tsx",
        "src/containers/public/main.tsx",
        "src/containers/public/Page.tsx",
        "src/pages/Extractor.tsx",
        "src/pages/Home.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
