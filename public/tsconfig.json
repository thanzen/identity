{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "files": [
        "events/events.ts",
        "libs/typings/amazeui-react.d.ts",
        "libs/typings/classnames.d.ts",
        "libs/typings/react-addons-update.d.ts",
        "libs/typings/react-bootstrap.d.ts",
        "libs/typings/react-dom.d.ts",
        "libs/typings/react-redux.d.ts",
        "libs/typings/react-router.d.ts",
        "libs/typings/react.d.ts",
        "libs/typings/redux.d.ts",
        "libs/typings/reselect.d.ts",
        "src/components/index.ts",
        "src/pages/index.ts",
        "src/BaseComponent.tsx",
        "src/components/Header.tsx",
        "src/components/PageContainer.tsx",
        "src/containers/Index.tsx",
        "src/containers/public/Index.tsx",
        "src/containers/public/main.tsx",
        "src/containers/public/Page.tsx",
        "src/pages/Home.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
